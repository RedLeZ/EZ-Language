env native;

friend native_math : cpp as nt;

// Integer arithmetic with functions
int add(int x, int y) {
    return x + y;
}

int multiply(int x, int y) {
    return x * y;
}

int a = 5;
int b = 7;

// Call user-defined functions
add(a, b);
multiply(a, b);

printf("5 + 7 = 12");
printf("5 * 7 = 35");

// Call native function
nt.mult(a, b);
printf("Native mult: 35");